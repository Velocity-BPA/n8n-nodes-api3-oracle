{
  "name": "n8n-nodes-api3-oracle",
  "version": "1.0.0",
  "description": "n8n community node for API3 Oracle",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/nodes/API3Oracle/API3Oracle.node.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "format": "prettier nodes credentials --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n-community-node-package",
    "api3",
    "oracle",
    "blockchain",
    "defi"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/API3OracleApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/API3Oracle/API3Oracle.node.js"
    ]
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "author": {
    "name": "Velocity BPA",
    "email": "support@velocitybpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/velocitybpa/n8n-nodes-api3-oracle.git"
  }
}